{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "supermarket-billing": {
      "projectType": "application",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/supermarket-billing",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico"],
            "styles": ["src/styles.css"]
          }
        },
        "serve": {
          "options": {
            "browserTarget": "supermarket-billing:build"
          }
        }
      }
    }
  },
  "defaultProject": "supermarket-billing"
}
